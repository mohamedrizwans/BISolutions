function doGet(e) {
  return HtmlService.createHtmlOutputFromFile("main");
}

function AddRecord(
  EnquiryNo,
  CustomerName,
  Salesman,
  EnquiryDate,
  FinalRateSP,
  FinalRateCustomer,
  QuotationNo,
  QuotationDate,
  RecDateCustBKConfirm,
  SentDateSPBKConfirm,
  BKConfirmNo,
  JobNo,
  CROReceivedDate,
  CROSenttoTransDate,
  CRONo,
  ContrNo,
  ContrNoRecDate,
  WBillNo,
  WBillSentDate,
  SealNo,
  SealNoRecDate,
  FullMovedtoTerminal,
  ShippingInstRecDate,
  ShipInstSentDatetoSL,
  ManifestRecDatefromSL,
  ShiperCLDocDate,
  BLDraftRecDatefromSL,
  BLDraftSentDatetoCust,
  BLDraftConfRecDatefromCust,
  DocSenttoBrokerdate,
  ManifestSenttoBrokerDate,
  GatePassRecDate,
  GatePassSenttoTransDate,
  FinalBayanOKtoLoadRecDate,
  BKMsgSentDate,
  BKconfirmationRecDate,
  LoadingConfirmationRecDate,
  PrealertSentDate,
  ArrivalNotificationSentDate,
  InvoicPreparedDate,
  DocumentsReleasedDate,
  GuaranteeCollectedDate,
  OBLCollectedDate,
  PaymentRecDate,
  HBLDate,
  MBLDate,
  DebitCreditNoteDate,
  TelexReleaseSeawayBillDate
) {
  var url =
    "https://docs.google.com/spreadsheets/d/1dJXL3OLniLD7aFCTDsLFGHixtp5oa7aFRB1fzu8ufKI/edit#gid=0"; //Paste URL of GOOGLE SHEET
  var ss = SpreadsheetApp.openByUrl(url);
  var webAppSheet = ss.getSheetByName("main");
  webAppSheet.appendRow([
    EnquiryNo,
    CustomerName,
    Salesman,
    EnquiryDate,
    FinalRateSP,
    FinalRateCustomer,
    QuotationNo,
    QuotationDate,
    RecDateCustBKConfirm,
    SentDateSPBKConfirm,
    BKConfirmNo,
    JobNo,
    CROReceivedDate,
    CROSenttoTransDate,
    CRONo,
    ContrNo,
    ContrNoRecDate,
    WBillNo,
    WBillSentDate,
    SealNo,
    SealNoRecDate,
    FullMovedtoTerminal,
    ShippingInstRecDate,
    ShipInstSentDatetoSL,
    ManifestRecDatefromSL,
    ShiperCLDocDate,
    BLDraftRecDatefromSL,
    BLDraftSentDatetoCust,
    BLDraftConfRecDatefromCust,
    DocSenttoBrokerdate,
    ManifestSenttoBrokerDate,
    GatePassRecDate,
    GatePassSenttoTransDate,
    FinalBayanOKtoLoadRecDate,
    BKMsgSentDate,
    BKconfirmationRecDate,
    LoadingConfirmationRecDate,
    PrealertSentDate,
    ArrivalNotificationSentDate,
    InvoicPreparedDate,
    DocumentsReleasedDate,
    GuaranteeCollectedDate,
    OBLCollectedDate,
    PaymentRecDate,
    HBLDate,
    MBLDate,
    DebitCreditNoteDate,
    TelexReleaseSeawayBillDate,
  ]);
}
